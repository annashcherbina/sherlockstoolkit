<%= render(partial: 'layouts/error_messages', locals: {object: @relationship}) %>

<%= form_for(@relationship) do |f| %>

    <div class="field">
        <%= f.label :family_id %><br>
        <%= f.number_field :family_id %>
    </div>
    <div class="field">
        <%= f.label :person_id %><br>
        <%= f.number_field :person_id %>
    </div>
    <div class="field">
        <%= f.label :person2_id %><br>
        <%= f.number_field :person2_id %>
    </div>
    <div class="field">
        <%= f.label :relation %><br>
        <%= f.text_field :relation %>
    </div>
    <div class="actions">
        <%= f.submit %>
    </div>
<% end %>
