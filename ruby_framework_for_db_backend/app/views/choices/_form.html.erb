<%= render(partial: 'layouts/error_messages', locals: {object: @choice}) %>

<%= form_for(@choice) do |f| %>

    <div class="field">
        <%= f.label :attribute_id %><br>
        <%= f.number_field :attribute_id %>
    </div>
    <div class="field">
        <%= f.label :name %><br>
        <%= f.text_field :name %>
    </div>
    <div class="actions">
        <%= f.submit %>
    </div>
<% end %>
