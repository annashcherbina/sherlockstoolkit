<h1>Panel Loci</h1>

<table class="display dataTable" cellspacing="0" width="100%">

  <thead>
    <tr>
      <th>Panel</th>
      <th>Locus</th>
      <th>Exclude</th>
      <th># Ambiguous</th>
      <th># Low</th>
      <th># Strand bias</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @panel_loci.each do |panel_locus| %>
      <tr>
        <td><%= @panels_hash[panel_locus.panel_id].name if panel_locus.panel_id %></td>
        <td><%= @locus_hash[panel_locus.locus_id].name if panel_locus.locus_id %></td>
        <td><%= panel_locus.exclude %></td>
        <td><%= panel_locus.ambiguous %></td>
        <td><%= panel_locus.low %></td>
        <td><%= panel_locus.strand_bias %></td>
      </tr>
    <% end %>
  </tbody>
</table>