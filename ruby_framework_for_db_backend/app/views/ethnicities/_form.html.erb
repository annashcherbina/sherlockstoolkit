<%= render(partial: 'layouts/error_messages', locals: {object: @ethnicity}) %>

<%= form_for(@ethnicity) do |f| %>

    <div class="field">
        <%= f.label :name %><br>
        <%= f.text_field :name %>
    </div>
    <div class="field">
        <%= f.label :race %><br>
        <%= f.text_field :race %>
    </div>
    <div class="actions">
        <%= f.submit %>
    </div>
<% end %>
